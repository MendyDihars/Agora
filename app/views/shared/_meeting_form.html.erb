<%= simple_form_for Meeting.new, url: teacher_meetings_path(@teacher), method: :post do |f| %>
  <%= f.input :skill_id, collection: @teacher.own_skills, selected: 1  %>
  <%= f.input :happen_at, html5: true, label: 'Propose a date' %>
  <%= f.input :message  %>
  <%= f.button :submit, value: 'Propose meeting', class: 'btn contact-button' %>
<% end %>
